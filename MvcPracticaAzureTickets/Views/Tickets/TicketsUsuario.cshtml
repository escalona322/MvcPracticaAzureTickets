@model List<Ticket> 
<h1>
    Tickets Usuario
</h1>
<a asp-controller="Tickets"
   asp-action="CrearTicket">
    Nuevo Ticket
</a>
<table class="table table-dark">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Importe</th>
            <th>Producto</th>
            <th>FileName</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Ticket ticket in Model)
            {
                <tr>
                    <td>
                         @ticket.Fecha
                    </td>
                    <td>
                        @ticket.Importe
                    </td>
                    <td>
                        @ticket.Producto
                    </td>
                    <td>
                        @ticket.FileName
                    </td>
                    <td>
                        <form method="post">
                            <button type="submit">
                                Procesar Ticket
                            </button>
                        </form>
                    </td>
                </tr>
            }
    </tbody>
</table>